<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body ng-app="myApp">
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Global Delivery Dashboard", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Overview", "Index", "Home")</li>
                    <li>@Html.ActionLink("Detail", "Detail", "Home")</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
    </div>

    <script type="text/javascript" src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/dashboard/d3.v3.js" type="text/javascript"></script>
    <script type="text/javascript" src="~/Scripts/dashboard/d3.js"></script>
    <script src="~/Scripts/dashboard/topojson.v0.min.js" type="text/javascript"></script>
   
    <script src="~/Scripts/dashboard/world.js" type="text/javascript"></script>
    <script src="~/Scripts/dashboard/d3-legend.min.js"></script>
    <script type="text/javascript" src="~/Scripts/dashboard/crossfilter.js"></script>
    <script type="text/javascript" src="~/Scripts/dashboard/queue.js"></script>
    <script type="text/javascript" src="~/Scripts/dashboard/angular.js"></script>
    <script type="text/javascript" src="~/Scripts/dashboard/PieChart.js"></script>
    <script type="text/javascript" src="~/Scripts/dashboard/WorldMap.js"></script>
    <script type="text/javascript" src="~/Scripts/dashboard/application.js"></script>
    <script type="text/javascript" src="~/Scripts/dashboard/PieChartDirective.js"></script>
    <script type="text/javascript" src="~/Scripts/dashboard/WorldMapDirective.js"></script>
    <script type="text/javascript" src="~/Scripts/dashboard/dataService.js"></script>
    <script type="text/javascript" src="~/Scripts/dashboard/optionService.js"></script>
    <script type="text/javascript" src="~/Scripts/dashboard/controller.js"></script>


























</body>
</html>
